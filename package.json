{
  "name": "hypergraph-desktop",
  "productName": "Hypergraph (Beta)",
  "author": "Liberate Science GmbH",
  "version": "0.8.0",
  "description": "Hypergraph Desktop app",
  "license": "MIT",
  "repository": "hypergraph-xyz/desktop",
  "scripts": {
    "build": "webpack -p",
    "dist": "electron-builder",
    "pack": "electron-builder --dir",
    "release": "np --no-publish --no-yarn --no-cleanup --any-branch",
    "start": "webpack-dev-server",
    "start:main": "electron .",
    "test": "prettier-standard && standard && npm run test:integration",
    "test:integration": "tap --no-cov",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@hypergraph-xyz/wikidata-identifiers": "^1.2.0",
    "@p2pcommons/sdk-js": "github:p2pcommons/sdk-js#6fbc85937063257e8d70e81f7f303b3f2dfe2c07",
    "adm-zip": "^0.4.16",
    "chmodr": "^1.0.2",
    "dat-encoding": "^5.0.1",
    "del": "^5.1.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-log": "^4.2.4",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.3.5",
    "focus-visible": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "is-online": "^8.4.0",
    "node-fetch": "^2.6.0",
    "p-timeout": "^3.2.0",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-spring": "^8.0.27",
    "reactour": "^1.18.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@svgr/webpack": "^5.2.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-builder-notarize": "^1.2.0",
    "file-loader": "^6.0.0",
    "np": "^6.3.2",
    "prettier-standard": "^16.4.1",
    "spectron": "^11.1.0",
    "standard": "^14.3.4",
    "tap": "^14.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.0"
  },
  "build": {
    "appId": "org.libscie.hypergraph",
    "files": [
      "**/*",
      "build/**/*"
    ],
    "linux": {
      "target": "AppImage"
    },
    "dmg": {
      "window": {
        "width": "416",
        "height": "200"
      },
      "iconSize": "64",
      "contents": [
        {
          "x": 100,
          "y": 100,
          "type": "file",
          "name": ""
        },
        {
          "x": 320,
          "y": 100,
          "type": "link",
          "path": "/Applications",
          "name": ""
        }
      ]
    },
    "afterSign": "electron-builder-notarize",
    "mac": {
      "hardenedRuntime": true
    },
    "protocols": {
      "name": "Hypergraph",
      "schemes": [
        "hypergraph"
      ]
    }
  }
}
